{
  "name": "gatsby-remark-embed-video-lite",
  "version": "0.1.1",
  "description": "Embed Videos (Youtube, Vimeo) in Gatsby via Markdown, using lite web components",
  "main": "index.js",
  "author": "Ahmad Awais <https://twitter.com/MrAhmadAwais/>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "remark",
    "markdown",
    "gatsby-remark-embed-video"
  ],
  "dependencies": {
    "@justinribeiro/lite-youtube": "^0.8.0",
    "@slightlyoff/lite-vimeo": "^0.1.1",
    "@types/node": "^14",
    "get-video-id": "^3.1.0",
    "unist-util-visit": "^1.3.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "auto-changelog": "^1.8.0",
    "cross-env": "^7",
    "react": "^16.13.1",
    "release-it": "^7.6.1",
    "typescript": "^3"
  },
  "homepage": "https://github.com/ahmadawais/gatsby-remark-better-embed-video#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmadawais/gatsby-remark-better-embed-video.git"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": ".clTemplate.hbs",
    "commitLimit": false,
    "package": true,
    "replaceText": {
      "FIX: ": "",
      "NEW: ": "",
      "IMPROVE: ": "",
      "DOC: ": ""
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "watch": "tsc -w",
    "log": "auto-changelog && git add . && git ci -m ' ðŸ“– DOC: Update the changelog' && git push",
    "release": "release-it",
    "release-minor": "release-it minor",
    "release-major": "release-it major",
    "release-dry": "release-it --dry-run",
    "release-beta": "release-it --preRelease=beta"
  }
}
